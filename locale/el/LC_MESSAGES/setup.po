msgid "yes"
msgstr "ναι"

#: pyload/setup/Setup.py:393
msgid "true"
msgstr "σωστό"

#: pyload/setup/Setup.py:393
msgid "t"
msgstr "t"

#: pyload/setup/Setup.py:396
msgid "no"
msgstr "όχι"

#: pyload/setup/Setup.py:396
msgid "false"
msgstr "λάθος"

#: pyload/setup/Setup.py:396
msgid "f"
msgstr "f"

