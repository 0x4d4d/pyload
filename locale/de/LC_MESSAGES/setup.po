msgstr "1 - Erstelle/Bearbeite Nutzer"

#: pyload/Setup.py:280
msgid "2 - List users"
msgstr "2 - Liste Nutzer auf"

#: pyload/Setup.py:281
msgid "3 - Remove user"
msgstr "3 - Entferne Nutzer"

#: pyload/Setup.py:282
msgid "4 - Quit"
msgstr "4 - Verlassen"

#: pyload/Setup.py:294
msgid "Users"
msgstr "Nutzer"

